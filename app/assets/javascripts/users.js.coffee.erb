previous_data = null

update_totals = ->
  request = $.get "/api/tree/values"
  request.success (data) -> insertValues(data)

unless $('ul.seed_values').length == 0
  setInterval ->
    update_totals()
  , 1500

insertValues = (data) ->
  if data != previous_data
    previous_data = data
    console.log(data)
    $('h2.participants').text(data.clicks) unless data.clicks == null
    $('h2.total_raised').text('$' + data.tree) unless data.tree == null
